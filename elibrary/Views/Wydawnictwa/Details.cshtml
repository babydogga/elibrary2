@model elibrary.Models.Wydawnictwo

@{
    ViewData["Title"] = "Details";
}

<div>
    <div class="row text">
        <div class="col-md-8 offset-2">
            <p>
                <h1>Szczegóły wydawnictwa</h1>
            </p>
            <div class="row">
                <div class="col-md offset-2">
                    <div class="form-group mb-3">
                        <label asp-for="WydPictureURL" class="conrol-label"></label>
                        <input asp-for="WydPictureURL" class="form-control" readonly />
                    </div>
                    <div class="form-group mb-3">
                        <label asp-for="NameWyd" class="conrol-label"></label>
                        <input asp-for="NameWyd" class="form-control" readonly />
                    </div>
                    <div class="form-group mb-3">
                        <label asp-for="DescWyd" class="conrol-label"></label>
                        <input asp-for="DescWyd" class="form-control" readonly />
                    </div>
                    <div class="d-flex justify-content-between">
                        <a class="btn btn-outline-secondary" asp-action="Index">Anuluj</a>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model?.Id">Edit</a> |
    <a asp-action="Index">Back to List</a>
</div>
